{
  "name": "rmzamora/admin-bundle",
  "type": "symfony-bundle",
  "description": "Fork of Symfony SonataAdminBundle",
  "keywords": ["Admin Generator", "admin", "sonata", "bootstrap", "rmzamora fork"],
  "homepage": "http://sonata-project.org/bundles/admin",
  "license": "MIT",
  "authors": [
    {
      "name": "Mell M. Zamora",
      "email": "me@mellzamora.com",
      "homepage": "http://mellzamora.com"
    },
    {
      "name": "Thomas Rabaix",
      "email": "thomas.rabaix@sonata-project.org",
      "homepage": "http://sonata-project.org"
    },
    {
      "name": "Sonata Community",
      "homepage": "https://github.com/sonata-project/SonataAdminBundle/contributors"
    }
  ],
  "require": {
    "symfony/http-foundation": "~2.4",
    "symfony/form": "~2.4",
    "symfony/validator": "~2.4",
    "symfony/security-bundle": "~2.4",
    "symfony/routing": "~2.4",
    "symfony/config": "~2.4",
    "symfony/console": "~2.4",
    "symfony/twig-bridge": "~2.4",
    "symfony/class-loader":"~2.4",
    "sensio/generator-bundle": "~2.3",
    "symfony/translation":"~2.3",
    "symfony/dependency-injection": "~2.3,>=2.3.3",
    "twig/twig": "~1.15",
    "twig/extensions": "~1.0",
    "knplabs/knp-menu-bundle": "~2.0",
    "sonata-project/exporter": "~1.0",
    "sonata-project/core-bundle": "dev-master",
    "rmzamora/block-bundle": "~2.3",
    "doctrine/common": "~2.4"
  },
  "require-dev": {
    "jms/translation-bundle": "~1.1",
    "symfony/yaml": "~2.3",
    "sonata-project/intl-bundle": "~2.1",
    "symfony/phpunit-bridge": "2.7.*@dev"
  },
  "suggest": {
    "jms/translation-bundle": "Extract message keys from Admins",
    "sonata-project/intl-bundle": "Add localized date and number into the list"
  },
  "autoload": {
    "psr-4": { "Sonata\\AdminBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-2.3": "2.3.x-dev"
    }
